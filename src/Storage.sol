pragma solidity >=0.7.0 <0.9.0;


contract Storage {

    uint256 number;

    //the function enter value to variable number
    function store(uint256 num) public {
        number = num;
    }

    //the function return the variable number
    function retrieve() public view returns (uint256){
        return number;
    }
}
